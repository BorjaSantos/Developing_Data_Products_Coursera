corr.matrix.gen.3vars <- function(N,coefficients,semilla){
set.seed(semilla)
# CRITERIOS DE CONTROL DE LOS PARMETROS DE LA FUNCION
if(N <=0){
stop("El tamano de la muestra aleatoria de matrices de correlacion no es correcta")
